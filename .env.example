# Enviroment variables needed to connect to the LiveKit server.
LIVEKIT_URL=
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=

# MCP server URL for booking/calendar integration (e.g. Zapier)
# Required format: https://mcp.zapier.com/api/v1/connect?token=YOUR_TOKEN
# Get your Zapier MCP token from: https://zapier.com/mcp
# Leave empty to disable MCP integration
MCP_SERVER_URL=

# Mem0 Platform API key for conversation memory (optional)
# Get a key from https://app.mem0.ai; leave empty to disable memory
MEM0_API_KEY=
